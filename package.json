{
  "name": "@pharmatech/sdk",
  "version": "0.4.12",
  "description": "PharmaTech Core API SDK",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "lint": "tsc",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "check-exports": "attw --pack . --profile esm-only",
    "test": "vitest run",
    "prepare": "husky"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json",
    "./types": {
      "import": "./dist/index.d.ts",
      "require": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/PharmaTechVe/pharmatech-sdk"
  },
  "keywords": [
    "typescript"
  ],
  "author": "PharmaTech <1001.27198200.ucla@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PharmaTechVe/pharmatech-sdk/issues"
  },
  "homepage": "https://github.com/PharmaTechVe/pharmatech-sdk",
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.4",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "vitest": "^3.0.6"
  },
  "dependencies": {
    "axios": "^1.8.1"
  }
}
